<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:orientation="vertical"
        android:id="@+id/linLayoutRating"
        tools:context=".ResultActivityFragments.RatingFragment">

        <LinearLayout
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:background="#00FFFFFF"
                android:layout_width="match_parent"
                android:id="@+id/contentframe">


            <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    app:cardCornerRadius="40dp"
                    android:layout_marginBottom="20dp"
                    app:cardElevation="4dp"
                    android:visibility="gone"
                    android:id="@+id/verdictCard"
                    android:layout_marginTop="20dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:layout_height="200dp">
                <LinearLayout
                        android:layout_width="match_parent"
                        android:orientation="vertical"
                        android:paddingBottom="10dp"
                        android:gravity="center"
                        android:layout_height="match_parent">
                    <FrameLayout
                            android:layout_width="match_parent"
                            android:orientation="horizontal"
                            android:layout_height="wrap_content">
                        <TextView
                                android:layout_width="wrap_content"
                                android:text="Verdict"
                                android:textSize="24dp"
                                android:fontFamily="@font/raleway_bold"
                                android:textColor="#6264c8"
                                android:layout_marginTop="40dp"
                                android:layout_marginLeft="20dp"
                                android:layout_height="wrap_content"/>
                        <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_gravity="end|bottom"
                                android:layout_marginBottom="5dp"
                                android:layout_marginRight="20dp"
                                android:id="@+id/pbar"
                                android:layout_height="wrap_content">
                            <ProgressBar
                                    android:layout_width="14dp"
                                    android:layout_gravity="center"
                                    android:paddingTop="3dp"
                                    android:layout_height="match_parent"/>

                            <TextView
                                    android:layout_width="match_parent"
                                    android:text="Loading ..."
                                    android:textSize="14sp"
                                    android:fontFamily="@font/raleway_light"
                                    android:textColor="#000"
                                    android:layout_height="wrap_content"/>
                        </LinearLayout>
                        <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_gravity="end|bottom"
                                android:id="@+id/pbar2"
                                android:layout_marginBottom="5dp"
                                android:layout_marginRight="20dp"
                                android:layout_height="wrap_content">
                            <ProgressBar
                                    android:layout_width="14dp"
                                    android:layout_gravity="center"
                                    android:paddingTop="3dp"
                                    android:layout_height="match_parent"/>

                            <TextView
                                    android:layout_width="match_parent"
                                    android:text="Loading ..."
                                    android:textSize="14sp"
                                    android:fontFamily="@font/raleway_light"
                                    android:textColor="#000"
                                    android:layout_height="wrap_content"/>
                        </LinearLayout>
                        <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_gravity="end|bottom"
                                android:layout_marginBottom="5dp"
                                android:layout_marginRight="20dp"
                                android:id="@+id/pbar3"
                                android:layout_height="wrap_content">
                            <ProgressBar
                                    android:layout_width="14dp"
                                    android:layout_gravity="center"
                                    android:paddingTop="3dp"
                                    android:layout_height="match_parent"/>

                            <TextView
                                    android:layout_width="match_parent"
                                    android:text="Loading ..."
                                    android:textSize="14sp"
                                    android:fontFamily="@font/raleway_light"
                                    android:textColor="#000"
                                    android:layout_height="wrap_content"/>
                        </LinearLayout>
                    </FrameLayout>
                    <LinearLayout
                            android:layout_width="match_parent"
                            android:orientation="horizontal"
                            android:gravity="center"
                            android:layout_height="match_parent">
                        <com.github.mikephil.charting.charts.PieChart
                                android:layout_width="0dp"
                                android:layout_weight="0.5"
                                android:id="@+id/pieChartGod"
                                android:layout_gravity="center"
                                android:layout_marginTop="20dp"
                                android:layout_marginBottom="20dp"
                                android:layout_height="102dp"/>
                        <View android:layout_width="1dp"
                                android:background="#7A7676"
                                android:layout_marginTop="20dp"
                                android:layout_marginBottom="20dp"
                                android:layout_height="match_parent"/>

                        <LinearLayout
                                android:layout_width="0dp"
                                android:layout_weight="0.5"
                                android:gravity="center"
                                android:orientation="vertical"
                                android:layout_height="match_parent">
                            <RatingBar
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:numStars="5"
                                    android:scaleX="1"
                                    android:scaleY="1"
                                    android:id="@+id/ratingGod"
                                    android:rating="3.5"
                                    android:layout_gravity="center"
                                    style="?android:attr/ratingBarStyleSmall"
                                    android:stepSize="0.1"
                                    android:theme="@style/RatingBar2"/>

                            <TextView
                                    android:layout_width="match_parent"
                                    android:text="3.5"
                                    android:gravity="center"
                                    android:textSize="32sp"
                                    android:id="@+id/finalRating"
                                    android:layout_gravity="center"
                                    android:textColor="#1A921F"
                                    android:fontFamily="@font/raleway_bold"
                                    android:layout_height="wrap_content"/>


                            <TextView
                                    android:layout_width="match_parent"
                                    android:text="Surely worth a try :)"
                                    android:gravity="center"
                                    android:textSize="10sp"
                                    android:layout_marginTop="20dp"
                                    android:layout_gravity="center"
                                    android:textColor="#1A1919"
                                    android:fontFamily="@font/raleway_light"
                                    android:layout_height="wrap_content"/>

                        </LinearLayout>

                    </LinearLayout>
                </LinearLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    app:cardCornerRadius="40dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:id="@+id/ratingCard"
                    android:visibility="gone"
                    android:layout_marginBottom="20dp"
                    android:layout_gravity="bottom|top"
                    android:layout_height="wrap_content">
                <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingBottom="40dp"
                        android:orientation="vertical">
                    <TextView
                            android:layout_width="wrap_content"
                            android:text="Rating Distribution"
                            android:textSize="24dp"
                            android:fontFamily="@font/raleway_bold"
                            android:textColor="#6264c8"
                            android:layout_marginTop="40dp"
                            android:layout_marginLeft="20dp"
                            android:layout_height="wrap_content"/>
                    <LinearLayout
                            android:layout_width="match_parent"
                            android:orientation="horizontal"
                            android:layout_height="wrap_content">

                        <androidx.cardview.widget.CardView
                                android:layout_marginLeft="20dp"
                                android:layout_marginRight="20dp"
                                android:layout_marginTop="20dp"
                                android:layout_width="0dp"
                                android:layout_marginBottom="20dp"
                                android:layout_weight="0.7"
                                app:cardBackgroundColor="#4CAF50"
                                app:cardElevation="4dp"
                                android:id="@+id/bookContainer"
                                app:contentPadding="10dp"
                                app:cardCornerRadius="40dp"
                                android:layout_height="100dp">
                            <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:orientation="horizontal">
                                <ProgressBar
                                        android:id="@+id/progressBar"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:visibility="gone"
                                        android:layout_gravity="center"
                                        />
                                <LinearLayout
                                        android:layout_width="0dp"
                                        android:layout_weight="0.2"
                                        android:orientation="horizontal"
                                        android:layout_height="match_parent">
                                    <androidx.cardview.widget.CardView
                                            android:layout_height="60dp"
                                            android:layout_width="60dp"
                                            app:cardElevation="0dp"
                                            app:cardCornerRadius="40dp"
                                            app:cardBackgroundColor="#fff"
                                            android:layout_marginLeft="14dp"
                                            android:layout_gravity="center">
                                        <ImageView
                                                android:layout_width="30dp"
                                                android:layout_gravity="center"
                                                android:id="@+id/goodReadsLogo"
                                                android:src="@drawable/goodreadslogo"
                                                android:layout_height="30dp"/>
                                    </androidx.cardview.widget.CardView>
                                    <LinearLayout
                                            android:layout_width="0dp"
                                            android:layout_weight="0.1"
                                            android:paddingLeft="10dp"
                                            android:orientation="vertical"
                                            android:layout_height="match_parent">
                                        <TextView
                                                android:layout_width="match_parent"
                                                android:gravity="bottom"
                                                android:fontFamily="@font/raleway_bold"
                                                android:text="Goodreads"
                                                android:textColor="#fff"
                                                android:layout_weight="0.5"
                                                android:layout_height="0dp"/>

                                        <TextView
                                                android:layout_width="match_parent"
                                                android:gravity="top"
                                                android:id="@+id/reviewCount"
                                                android:fontFamily="@font/raleway_light"
                                                android:text="3.5"
                                                android:textColor="#fff"
                                                android:layout_weight="0.5"
                                                android:layout_height="0dp"/>
                                    </LinearLayout>
                                </LinearLayout>
                                <LinearLayout
                                        android:layout_width="wrap_content"
                                        android:orientation="vertical"
                                        android:gravity="center"
                                        android:paddingTop="4.5dp"
                                        android:layout_height="match_parent">

                                    <RatingBar
                                            android:id="@+id/rating"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:numStars="5"
                                            android:scaleX="1"
                                            android:scaleY="1"
                                            android:rating="3.5"
                                            android:layout_gravity="center"
                                            style="?android:attr/ratingBarStyleSmall"
                                            android:stepSize="0.1"
                                            android:theme="@style/RatingBar"/>

                                    <TextView
                                            android:layout_width="match_parent"
                                            android:gravity="top|center"
                                            android:id="@+id/avRate"
                                            android:fontFamily="@font/raleway_bold"
                                            android:text="3.5"
                                            android:textColor="#fff"
                                            android:layout_height="wrap_content"/>
                                </LinearLayout>
                            </LinearLayout>
                        </androidx.cardview.widget.CardView>
                    </LinearLayout>
                    <LinearLayout
                            android:layout_width="match_parent"
                            android:orientation="horizontal"
                            android:layout_height="wrap_content">
                        <androidx.cardview.widget.CardView
                                android:layout_marginLeft="20dp"
                                android:layout_marginRight="20dp"
                                android:layout_marginTop="20dp"
                                android:layout_width="0dp"
                                android:layout_weight="0.7"
                                android:layout_marginBottom="20dp"
                                app:cardBackgroundColor="#4CAF50"
                                app:cardElevation="4dp"
                                android:id="@+id/bookContainer2"
                                app:contentPadding="10dp"
                                app:cardCornerRadius="40dp"
                                android:layout_height="100dp">
                            <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:orientation="horizontal">
                                <ProgressBar
                                        android:id="@+id/progressBar2"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:visibility="gone"
                                        android:layout_gravity="center"
                                        />
                                <LinearLayout
                                        android:layout_width="0dp"
                                        android:layout_weight="0.2"
                                        android:orientation="horizontal"
                                        android:layout_height="match_parent">
                                    <androidx.cardview.widget.CardView
                                            android:layout_height="60dp"
                                            android:layout_width="60dp"
                                            app:cardElevation="0dp"
                                            app:cardCornerRadius="40dp"
                                            app:cardBackgroundColor="#fff"
                                            android:layout_marginLeft="14dp"
                                            android:layout_gravity="center">
                                        <ImageView
                                                android:layout_width="30dp"
                                                android:layout_gravity="center"
                                                android:id="@+id/googleBooksLogo"
                                                android:src="@drawable/google"
                                                android:layout_height="30dp"/>
                                    </androidx.cardview.widget.CardView>
                                    <LinearLayout
                                            android:layout_width="0dp"
                                            android:layout_weight="0.1"
                                            android:paddingLeft="10dp"
                                            android:orientation="vertical"
                                            android:layout_height="match_parent">
                                        <TextView
                                                android:layout_width="match_parent"
                                                android:gravity="bottom"
                                                android:fontFamily="@font/raleway_bold"
                                                android:text="Google Books"
                                                android:textColor="#fff"
                                                android:layout_weight="0.5"
                                                android:layout_height="0dp"/>

                                        <TextView
                                                android:layout_width="match_parent"
                                                android:gravity="top"
                                                android:id="@+id/reviewCount2"
                                                android:fontFamily="@font/raleway_light"
                                                android:text="3.5"
                                                android:textColor="#fff"
                                                android:layout_weight="0.5"
                                                android:layout_height="0dp"/>
                                    </LinearLayout>
                                </LinearLayout>
                                <LinearLayout
                                        android:layout_width="wrap_content"
                                        android:orientation="vertical"
                                        android:gravity="center"
                                        android:paddingTop="4.5dp"
                                        android:layout_height="match_parent">

                                    <RatingBar
                                            android:id="@+id/rating2"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:numStars="5"
                                            android:scaleX="1"
                                            android:scaleY="1"
                                            android:rating="3.5"
                                            android:layout_gravity="center"
                                            style="?android:attr/ratingBarStyleSmall"
                                            android:stepSize="0.1"
                                            android:theme="@style/RatingBar"/>

                                    <TextView
                                            android:layout_width="match_parent"
                                            android:gravity="top|center"
                                            android:id="@+id/avRate2"
                                            android:fontFamily="@font/raleway_bold"
                                            android:text="3.5"
                                            android:textColor="#fff"
                                            android:layout_height="wrap_content"/>
                                </LinearLayout>
                            </LinearLayout>
                        </androidx.cardview.widget.CardView>
                    </LinearLayout>
                    <LinearLayout
                            android:layout_width="match_parent"
                            android:orientation="horizontal"
                            android:layout_height="wrap_content">
                        <androidx.cardview.widget.CardView
                                android:layout_marginLeft="20dp"
                                android:layout_marginRight="20dp"
                                android:layout_marginTop="20dp"
                                android:layout_width="0dp"
                                android:layout_weight="0.7"
                                android:layout_marginBottom="20dp"
                                app:cardBackgroundColor="#4CAF50"
                                app:cardElevation="4dp"
                                android:id="@+id/bookContainer3"
                                app:contentPadding="10dp"
                                app:cardCornerRadius="40dp"
                                android:layout_height="100dp">
                            <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:orientation="horizontal">
                                <LinearLayout
                                        android:layout_width="0dp"
                                        android:layout_weight="0.2"
                                        android:orientation="horizontal"
                                        android:layout_height="match_parent">
                                    <androidx.cardview.widget.CardView
                                            android:layout_height="60dp"
                                            android:layout_width="60dp"
                                            app:cardElevation="0dp"
                                            app:cardCornerRadius="40dp"
                                            app:cardBackgroundColor="#fff"
                                            android:layout_marginLeft="14dp"
                                            android:layout_gravity="center">
                                        <ImageView
                                                android:layout_width="30dp"
                                                android:layout_gravity="center"
                                                android:id="@+id/flipkartLogo"
                                                android:src="@drawable/flipkart"
                                                android:layout_height="30dp"/>
                                    </androidx.cardview.widget.CardView>
                                    <LinearLayout
                                            android:layout_width="0dp"
                                            android:layout_weight="0.1"
                                            android:paddingLeft="10dp"
                                            android:orientation="vertical"
                                            android:layout_height="match_parent">
                                        <TextView
                                                android:layout_width="match_parent"
                                                android:gravity="bottom"
                                                android:fontFamily="@font/raleway_bold"
                                                android:text="Flipkart"
                                                android:textColor="#fff"
                                                android:layout_weight="0.5"
                                                android:layout_height="0dp"/>

                                        <TextView
                                                android:layout_width="match_parent"
                                                android:gravity="top"
                                                android:id="@+id/reviewCount3"
                                                android:fontFamily="@font/raleway_light"
                                                android:text="3.5"
                                                android:textColor="#fff"
                                                android:layout_weight="0.5"
                                                android:layout_height="0dp"/>
                                    </LinearLayout>
                                </LinearLayout>
                                <LinearLayout
                                        android:layout_width="wrap_content"
                                        android:orientation="vertical"
                                        android:gravity="center"
                                        android:paddingTop="4.5dp"
                                        android:layout_height="match_parent">

                                    <RatingBar
                                            android:id="@+id/rating3"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:numStars="5"
                                            android:scaleX="1"
                                            android:scaleY="1"
                                            android:rating="3.5"
                                            android:layout_gravity="center"
                                            style="?android:attr/ratingBarStyleSmall"
                                            android:stepSize="0.1"
                                            android:theme="@style/RatingBar"/>

                                    <TextView
                                            android:layout_width="match_parent"
                                            android:gravity="top|center"
                                            android:id="@+id/avRate3"
                                            android:fontFamily="@font/raleway_bold"
                                            android:text="3.5"
                                            android:textColor="#fff"
                                            android:layout_height="wrap_content"/>
                                </LinearLayout>
                            </LinearLayout>
                        </androidx.cardview.widget.CardView>
                    </LinearLayout>
                    <LinearLayout
                            android:layout_width="match_parent"
                            android:orientation="horizontal"
                            android:layout_height="wrap_content">
                        <androidx.cardview.widget.CardView
                                android:layout_marginLeft="20dp"
                                android:layout_marginRight="20dp"
                                android:layout_marginTop="20dp"
                                android:layout_width="0dp"
                                android:layout_marginBottom="20dp"
                                android:layout_weight="0.7"
                                app:cardBackgroundColor="#4CAF50"
                                app:cardElevation="4dp"
                                android:id="@+id/bookContainer4"
                                app:contentPadding="10dp"
                                app:cardCornerRadius="40dp"
                                android:layout_height="100dp">
                            <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:orientation="horizontal">
                                <LinearLayout
                                        android:layout_width="0dp"
                                        android:layout_weight="0.2"
                                        android:orientation="horizontal"
                                        android:layout_height="match_parent">
                                    <androidx.cardview.widget.CardView
                                            android:layout_height="60dp"
                                            android:layout_width="60dp"
                                            app:cardElevation="0dp"
                                            app:cardCornerRadius="40dp"
                                            app:cardBackgroundColor="#fff"
                                            android:layout_marginLeft="14dp"
                                            android:layout_gravity="center">
                                        <ImageView
                                                android:layout_width="30dp"
                                                android:layout_gravity="center"
                                                android:src="@drawable/lbthing"
                                                android:layout_height="30dp"/>
                                    </androidx.cardview.widget.CardView>
                                    <LinearLayout
                                            android:layout_width="0dp"
                                            android:layout_weight="0.1"
                                            android:paddingLeft="10dp"
                                            android:orientation="vertical"
                                            android:layout_height="match_parent">
                                        <TextView
                                                android:layout_width="match_parent"
                                                android:gravity="bottom"
                                                android:fontFamily="@font/raleway_bold"
                                                android:text="Library Thing"
                                                android:textColor="#fff"
                                                android:layout_weight="0.5"
                                                android:layout_height="0dp"/>

                                        <TextView
                                                android:layout_width="match_parent"
                                                android:gravity="top"
                                                android:id="@+id/reviewCount4"
                                                android:fontFamily="@font/raleway_light"
                                                android:text="3.5"
                                                android:textColor="#fff"
                                                android:layout_weight="0.5"
                                                android:layout_height="0dp"/>
                                    </LinearLayout>
                                </LinearLayout>
                                <LinearLayout
                                        android:layout_width="wrap_content"
                                        android:orientation="vertical"
                                        android:gravity="center"
                                        android:paddingTop="4.5dp"
                                        android:layout_height="match_parent">

                                    <RatingBar
                                            android:id="@+id/rating4"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:numStars="5"
                                            android:scaleX="1"
                                            android:scaleY="1"
                                            android:rating="3.5"
                                            android:layout_gravity="center"
                                            style="?android:attr/ratingBarStyleSmall"
                                            android:stepSize="0.1"
                                            android:theme="@style/RatingBar"/>

                                    <TextView
                                            android:layout_width="match_parent"
                                            android:gravity="top|center"
                                            android:id="@+id/avRate4"
                                            android:fontFamily="@font/raleway_bold"
                                            android:text="3.5"
                                            android:textColor="#fff"
                                            android:layout_height="wrap_content"/>
                                </LinearLayout>
                            </LinearLayout>

                        </androidx.cardview.widget.CardView>
                    </LinearLayout>
                    <LinearLayout
                            android:layout_width="match_parent"
                            android:orientation="horizontal"
                            android:layout_height="wrap_content">
                        <androidx.cardview.widget.CardView
                                android:layout_marginLeft="20dp"
                                android:layout_marginRight="20dp"
                                android:layout_marginTop="20dp"
                                android:layout_width="0dp"
                                android:layout_marginBottom="20dp"
                                android:layout_weight="0.7"
                                app:cardBackgroundColor="#4CAF50"
                                app:cardElevation="4dp"
                                android:id="@+id/bookContainer5"
                                app:contentPadding="10dp"
                                app:cardCornerRadius="40dp"
                                android:layout_height="100dp">
                            <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:orientation="horizontal">
                                <LinearLayout
                                        android:layout_width="0dp"
                                        android:layout_weight="0.2"
                                        android:orientation="horizontal"
                                        android:layout_height="match_parent">
                                    <androidx.cardview.widget.CardView
                                            android:layout_height="60dp"
                                            android:layout_width="60dp"
                                            app:cardElevation="0dp"
                                            app:cardCornerRadius="40dp"
                                            app:cardBackgroundColor="#fff"
                                            android:layout_marginLeft="14dp"
                                            android:layout_gravity="center">
                                        <ImageView
                                                android:layout_width="70dp"
                                                android:layout_gravity="center"
                                                android:src="@drawable/walmart"
                                                android:layout_height="70dp"/>
                                    </androidx.cardview.widget.CardView>
                                    <LinearLayout
                                            android:layout_width="0dp"
                                            android:layout_weight="0.1"
                                            android:paddingLeft="10dp"
                                            android:orientation="vertical"
                                            android:layout_height="match_parent">
                                        <TextView
                                                android:layout_width="match_parent"
                                                android:gravity="bottom"
                                                android:fontFamily="@font/raleway_bold"
                                                android:text="Walmart"
                                                android:textColor="#fff"
                                                android:layout_weight="0.5"
                                                android:layout_height="0dp"/>

                                        <TextView
                                                android:layout_width="match_parent"
                                                android:gravity="top"
                                                android:id="@+id/reviewCount5"
                                                android:fontFamily="@font/raleway_light"
                                                android:text="3.5"
                                                android:textColor="#fff"
                                                android:layout_weight="0.5"
                                                android:layout_height="0dp"/>
                                    </LinearLayout>
                                </LinearLayout>
                                <LinearLayout
                                        android:layout_width="wrap_content"
                                        android:orientation="vertical"
                                        android:gravity="center"
                                        android:paddingTop="4.5dp"
                                        android:layout_height="match_parent">

                                    <RatingBar
                                            android:id="@+id/rating5"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:numStars="5"
                                            android:scaleX="1"
                                            android:scaleY="1"
                                            android:rating="3.5"
                                            android:layout_gravity="center"
                                            style="?android:attr/ratingBarStyleSmall"
                                            android:stepSize="0.1"
                                            android:theme="@style/RatingBar"/>

                                    <TextView
                                            android:layout_width="match_parent"
                                            android:gravity="top|center"
                                            android:id="@+id/avRate5"
                                            android:fontFamily="@font/raleway_bold"
                                            android:text="3.5"
                                            android:textColor="#fff"
                                            android:layout_height="wrap_content"/>
                                </LinearLayout>
